<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.easykt.easykt">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/kt_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/kt_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Splash"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/app_name"
            android:theme="@style/splashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".mctestActivity" />
        <activity android:name=".ActivityTutorial" />
        <activity android:name=".ActivitQuiz" />
        <activity android:name=".ActivityVideo" />
        <activity android:name=".ActivityFLTest"></activity>
    </application>

</manifest>